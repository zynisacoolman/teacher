<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="1360dp"
            android:background="@drawable/takemedicine_bac"
            android:orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="156dp" />
            <!--表头-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="123" />
                <!--注射单姓名-->
                <cn.jucheng.www.hulisiwei.customcontrols.FitHeightTextView
                    android:id="@+id/takemedicine_name"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="80"
                    android:gravity="center_vertical"
                    android:paddingLeft="6dp"
                    android:textColor="@color/heise"
                    android:textSize="@dimen/text_size16" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="42" />
                <!--注射单性别-->
                <cn.jucheng.www.hulisiwei.customcontrols.FitHeightTextView
                    android:id="@+id/takemedicine_sex"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="60"
                    android:gravity="center_vertical"
                    android:paddingLeft="6dp"
                    android:textColor="@color/heise"
                    android:textSize="@dimen/text_size16" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="51" />
                <!--注射单年龄-->
                <cn.jucheng.www.hulisiwei.customcontrols.FitHeightTextView
                    android:id="@+id/takemedicine_age"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="60"
                    android:gravity="center_vertical"
                    android:paddingLeft="6dp"
                    android:textColor="@color/heise"
                    android:textSize="@dimen/text_size16" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="51" />
                <!--注射单科别-->
                <cn.jucheng.www.hulisiwei.customcontrols.FitHeightTextView
                    android:id="@+id/takemedicine_division"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="100"
                    android:gravity="center_vertical"
                    android:paddingLeft="6dp"
                    android:textColor="@color/heise"
                    android:textSize="@dimen/text_size16" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="57" />
                <!--注射单床号-->
                <cn.jucheng.www.hulisiwei.customcontrols.FitHeightTextView
                    android:id="@+id/takemedicine_bednumber"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="80"
                    android:gravity="center_vertical"
                    android:paddingLeft="6dp"
                    android:textColor="@color/heise"
                    android:textSize="@dimen/text_size16" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="71" />
                <!--注射单病案号-->
                <cn.jucheng.www.hulisiwei.customcontrols.FitHeightTextView
                    android:id="@+id/take_medicalrecord_number"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="80"
                    android:gravity="center_vertical"
                    android:paddingLeft="6dp"
                    android:textColor="@color/heise"
                    android:textSize="@dimen/text_size16" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="85" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="66dp"
                android:orientation="horizontal">
            </LinearLayout>
            <!--内容-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="919dp"
                android:orientation="horizontal">

                <cn.jucheng.www.hulisiwei.base.MyList
                    android:id="@+id/takemedicine_list"
                    android:layout_width="match_parent"
                    android:layout_height="919dp"
                    android:divider="@color/touming"
                    android:dividerHeight="1dp"
                    android:listSelector="@color/touming" />

            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="13dp" />
            <!--结尾-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="84" />
                <!--年份-->
                <cn.jucheng.www.hulisiwei.customcontrols.FitHeightTextView
                    android:id="@+id/takemedicine_years"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="80"
                    android:gravity="center_vertical"
                    android:paddingLeft="6dp"
                    android:textColor="@color/heise"
                    android:textSize="@dimen/text_size16" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="29" />
                <!--月份-->
                <cn.jucheng.www.hulisiwei.customcontrols.FitHeightTextView
                    android:id="@+id/takemedicine_mouth"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="60"
                    android:gravity="center_vertical"
                    android:paddingLeft="6dp"
                    android:textColor="@color/heise"
                    android:textSize="@dimen/text_size16" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="30" />
                <!--日-->
                <cn.jucheng.www.hulisiwei.customcontrols.FitHeightTextView
                    android:id="@+id/takemedicine_day"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="60"
                    android:gravity="center_vertical"
                    android:paddingLeft="6dp"
                    android:textColor="@color/heise"
                    android:textSize="@dimen/text_size16" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="274" />
                <!--核对者-->
                <cn.jucheng.www.hulisiwei.customcontrols.FitHeightTextView
                    android:id="@+id/takemedicine_checkuser"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="80"
                    android:gravity="center_vertical"
                    android:paddingLeft="6dp"
                    android:textColor="@color/heise"
                    android:textSize="@dimen/text_size16" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="78" />
                <!--执行者-->
                <cn.jucheng.www.hulisiwei.customcontrols.FitHeightTextView
                    android:id="@+id/takemedicine_carryout"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="80"
                    android:gravity="center_vertical"
                    android:paddingLeft="6dp"
                    android:textColor="@color/heise"
                    android:textSize="@dimen/text_size16" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="85" />
            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="50dp" />
            <!--页数-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="446" />

                <cn.jucheng.www.hulisiwei.customcontrols.FitHeightTextView
                    android:id="@+id/takemedicine_page_number"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="50"
                    android:text="第1页"
                    android:textColor="@color/black_333"
                    android:textSize="@dimen/text_size14" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="444" />
            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="76dp" />

        </LinearLayout>
    </ScrollView>
</LinearLayout>