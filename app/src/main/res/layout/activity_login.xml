<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:myViewXmlns="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/dl_bg"
    android:orientation="vertical"
    tools:context="cn.jucheng.www.hulisiwei.LoginActivity" >

    <View
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="15" />

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="50"
        android:orientation="horizontal" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:text="发送的消息：" />

        <EditText
            android:id="@+id/etBingLiLeiXing"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:hint="病例类型" />

        <EditText
            android:id="@+id/etBingLiMingCheng"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:hint="病例名称" />

        <Button
            android:id="@+id/btnFSBingLiMingCheng"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:onClick="onTest"
            android:text="病例名称" />

        <EditText
            android:id="@+id/etPingJia"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:hint="评价" />

        <Button
            android:id="@+id/btnFSXianShiPingJia"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:onClick="onTest"
            android:text="显示评价" />

        <Button
            android:id="@+id/btnFSXianShiPingJia2"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:onClick="onTest"
            android:text="关闭评价" />

        <EditText
            android:id="@+id/etZhuangTaiId"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:hint="状态Id" />

        <Button
            android:id="@+id/btnFSBingQingBianHua"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:onClick="onTest"
            android:text="教师确认病情变化" />

        <EditText
            android:id="@+id/etXunLianZhuangTai"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:hint="训练状态" />

        <Button
            android:id="@+id/btnFSXunLianZhuangTaiJieShou"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:onClick="onTest"
            android:text="训练状态接受" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="50"
        android:orientation="horizontal" >

        <EditText
            android:id="@+id/etJiaoShiTiShi"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:hint="提示内容" />

        <Button
            android:id="@+id/btnFSJiaoShiTiShi"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:onClick="onTest"
            android:text="教师提示" />

        <Button
            android:id="@+id/btnFSSuoYaoXueShengJiZhuangTai"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:onClick="onTest"
            android:text="索要当前学生机状态" />

        <EditText
            android:id="@+id/etZhuangTai"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:hint="状态标识" />

        <EditText
            android:id="@+id/etBingLiLeiXing1"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:hint="病例类型" />

        <EditText
            android:id="@+id/etBingLiMingCheng1"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:hint="病例名称" />

        <Button
            android:id="@+id/btnFSXunLianZhuangTai"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:onClick="onTest"
            android:text="主动发送当前教师机的训练状态" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="50"
        android:orientation="horizontal" >

        <TextView
            android:layout_width="150dp"
            android:layout_height="fill_parent"
            android:text="接收的消息：" />

        <Button
            android:id="@+id/btnJSSheBeiZhuangTaiA0"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:onClick="onTest"
            android:text="设备状态00A0" />

        <Button
            android:id="@+id/btnJSSheBeiZhuangTaiAD"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:onClick="onTest"
            android:text="设备状态00AD" />

        <Button
            android:id="@+id/btnJSJiaoZhengSheBeiShiJian"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:onClick="onTest"
            android:text="校正设备时间" />

        <Button
            android:id="@+id/btnJSBingRenWanDaiXinXi"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:onClick="onTest"
            android:text="病人腕带信息" />

        <Button
            android:id="@+id/btnJSBiaoDan"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:onClick="onTest"
            android:text="学生填写的各种表单" />

        <Button
            android:id="@+id/btnJSGengGaiYiZhuZhuangTai"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:onClick="onTest"
            android:text="更改医嘱状态" />
    </LinearLayout>

    <EditText
        android:id="@+id/etXianShi"
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="100" />

    <View
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="15" />

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="40"
        android:orientation="horizontal" >

        <View
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="569" />

        <cn.jucheng.www.hulisiwei.customcontrols.FitHeightEditText
            android:id="@+id/etXingMing"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="200"
            android:background="@drawable/shape_biankuang"
            android:inputType="text"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:textColor="@color/denglu"
            android:textSize="@dimen/text_size18"
            myViewXmlns:attr2="2" />

        <View
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="300" />

        <cn.jucheng.www.hulisiwei.customcontrols.FitHeightButton
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="100"
            android:background="@drawable/s_btn1"
            android:onClick="onSettings"
            android:text="设置"
            android:textColor="@color/btnLogin_text"
            android:textSize="@dimen/text_size20"
            myViewXmlns:attr2="1" />

        <View
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="111" />
    </LinearLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="16" />

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="40"
        android:orientation="horizontal" >

        <View
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="569" />

        <cn.jucheng.www.hulisiwei.customcontrols.FitHeightEditText
            android:id="@+id/etXueHao"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="200"
            android:background="@drawable/shape_biankuang"
            android:inputType="text"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:textColor="@color/denglu"
            android:textSize="@dimen/text_size18"
            myViewXmlns:attr2="2" />

        <View
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="511" />
    </LinearLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="45" />

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="42"
        android:orientation="horizontal" >

        <View
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="509" />

        <cn.jucheng.www.hulisiwei.customcontrols.FitHeightButton
            android:id="@+id/btnDengLu"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="260"
            android:background="@drawable/s_btn1"
            android:onClick="onDengLu"
            android:text="@string/_denglu"
            android:textColor="@color/btnLogin_text"
            android:textSize="@dimen/text_size20"
            myViewXmlns:attr2="1" />

        <View
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="509" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="289" >
    </LinearLayout>

</LinearLayout>